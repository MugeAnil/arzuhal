<%= form_for :user, url: users_path do |f| %>

    <p>
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </p>
    <p>
      <%= f.label :surname %><br>
      <%= f.text_field :surname %>
    </p>
    <p>
      <%= f.label :e_mail %><br>
      <%= f.text_field :e_mail %>
    </p>
    <p>
      <%= f.label :text %><br>
      <%= f.text_area :text %>
    </p>
    <p>
      <%= f.submit %>
    </p>
    <% end %>